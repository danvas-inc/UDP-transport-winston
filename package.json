{
  "name": "udp-transport-winston",
  "version": "1.0.5",
  "description": "A winston transport for UDP in typescript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.json",
    "prepare": "rimraf dist && npm run lint:fix && npm run build",
    "lint": "eslint '*/**/*.{js,ts}' --quiet",
    "lint:fix": "eslint '*/**/*.{js,ts}' --quiet --fix",
    "test": "echo \"Error: no test specified\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leotolk/UDP-transport-winston"
  },
  "keywords": [
    "winston",
    "logstash",
    "splunk",
    "transport",
    "udp",
    "udp-transport-winston",
    "typescript"
  ],
  "author": "leotolk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/leotolk/UDP-transport-winston/issues"
  },
  "homepage": "https://github.com/leotolk/UDP-transport-winston#readme",
  "dependencies": {
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/node": "^14.0.9",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:fix"
    }
  }
